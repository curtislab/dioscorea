<script>
	var hideable = document.getElementById('hideable');
	var open = document.getElementById('btn-open');
	open.onclick = function () {
		if (hideable.style.display == "none") {
		    hideable.style.display = "block";
		}
		else {
			hideable.style.display = "none";
		}
	};
</script>

